<div class="user" id="userlist-entry-{senderId}">
    <img class="user-avatar" id="userlist-entry-avatar-{senderId}" src="{^avatar}/images/icons/user_64x64.png{/avatar}{avatar}" title="{nickname}" />
    <div class="userlist-entry-nickname" id="userlist-entry-nickname-{senderId}">
        {nickname}
        <span class="userlist-entry-typingstatus-normal" id="userlist-entry-typingstatus-{senderId}"></span>
    </div>
</div>